      "size": 15932
    },
    {
      "id": 30,
      "ar": "وَظِلّٖ مَّمۡدُودٖ",
      "en": "And shade extended",
      "path": "/audio/056/030.mp3",
      "dir": "/audio/056",
      "size": 14626
    },
    {
      "id": 31,
      "ar": "وَمَآءٖ مَّسۡكُوبٖ",
